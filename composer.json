{
    "name": "artisanry/chartjs",
    "description": "ChartJS Integration with a Fluent Builder for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-ChartJS",
        "Laravel ChartJS"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\ChartJS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\ChartJS\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\ChartJS\\ChartJSServiceProvider"
            ],
            "aliases": {
                "ChartJS": "Artisanry\\ChartJS\\Facades\\ChartJS"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
